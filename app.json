      "bundleIdentifier": "com.vibecode.mynotebooks"
    },
    "android": {
      "package": "com.vibecode.mynotebooks",
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "6e7d357f-e452-428f-b2a4-a866073e8df4"
      }
    }
  }
}
